<VBox alignment="CENTER" prefHeight="667.0" prefWidth="375.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uab.wmeeks.qr_gui.QR_galleryController">
