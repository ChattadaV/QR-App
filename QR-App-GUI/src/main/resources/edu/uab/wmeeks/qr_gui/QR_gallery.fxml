        <Label prefHeight="73.0" prefWidth="216.0" text="QR Code Gallery" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="System Bold" size="28.0" />
            </font>
        </Label>
        <Accordion />
        <Button mnemonicParsing="false" onAction="#switchToMenu" prefHeight="56.0" prefWidth="125.0" text="Back To Menu" />
        <ImageView fitHeight="112.0" fitWidth="156.0" pickOnBounds="true" preserveRatio="true" />
        <Label text="Name of QR code #1" />
        <ImageView fitHeight="112.0" fitWidth="156.0" pickOnBounds="true" preserveRatio="true" />
        <Label text="Name of QR code #2" />
        <ImageView fitHeight="112.0" fitWidth="156.0" pickOnBounds="true" preserveRatio="true" />
        <Label text="Name of QR code #3" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
